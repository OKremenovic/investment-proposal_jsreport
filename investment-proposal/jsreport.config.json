{
    "httpPort": 5498,
    "store": {
        "provider": "fs"
    },
    "blobStorage": {
        "provider": "fs"
    },
    "logger": {
        "console": {
            "transport": "console",
            "level": "debug"
        },
        "file": {
            "transport": "file",
            "level": "info",
            "filename": "logs/reporter.log"
        },
        "error": {
            "transport": "file",
            "level": "error",
            "filename": "logs/error.log"
        }
    },
    "trustUserCode": true,
    "reportTimeout": 60000,
    "workers": {
        "numberOfWorkers": 2
    },
    "extensions": {
        "authentication": {
            "cookieSession": {},
            "admin": {
                "username": "admin",
                "password": "password"
            },
            "enabled": false
        },
        "npm": {
            "allowedModules": "*"
        },
        "fs-store": {
            "externalModificationsSync": true
        }
    }
}
